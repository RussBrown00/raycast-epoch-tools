{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "version": "0.0.1",
  "name": "raycast-epoch-tools",
  "title": "Raycast Epoch Tools",
  "description": "Epoch timestamp related quick tools for Raycast",
  "icon": "command-icon.png",
  "author": "Russell Brown <russ@902labs.com>",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Copy Epoch TS to clipboard",
      "description": "Copy epoch timestamp to clipboard",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.77.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
